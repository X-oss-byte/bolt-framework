{
  "name": "@gluestack-v2/seal-framework",
  "version": "0.0.1",
  "description": "Seal Framework & VM CLI Tools",
  "private": true,
  "scripts": {
    "install:all": "npm install --workspaces --if-present",
    "link:all": "npm link --workspaces --if-present && npm run fix:permission",
    "build:all": "npm run build --workspaces --if-present",
    "watch:sealvm": "npm run watch --workspace @gluestack-v2/sealvm",
    "watch:seal": "npm run watch --workspace @gluestack-v2/seal",
    "fix:permission": "chmod +x ./packages/sealvm/build/index.js && chmod +x ./packages/seal/build/index.js"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluestack-v2/seal-framework.git"
  },
  "keywords": [
    "gluestack",
    "v2",
    "seal",
    "sealvm",
    "cli"
  ],
  "author": "Gluestack.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluestack-v2/seal-framework/issues"
  },
  "homepage": "https://github.com/gluestack-v2/seal-framework#readme"
}
